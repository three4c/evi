{
  "manifest_version": 3,
  "name": "evi",
  "version": "1.0.0",
  "permissions": [
    "storage",
    "clipboardRead"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "js": [
        "assets/index.tsx-loader-BddfZ45V.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/shortcuts-Bdqo3K1J.js",
        "assets/App-BF_L1re2.js",
        "assets/index.tsx-C3X2j33V.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
